{
  "manifest_version": 3,
  "name": "Autofiller Extension",
  "version": "2.0",
  "description": "Password Manager",
  "permissions": [
    "storage", "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["autofiller.js"]
    }
  ]
}